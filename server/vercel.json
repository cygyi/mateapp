{
  "version": 2,
  "regions": [
    "hkg1"
  ],
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "github": {
    "silent": true
  },
  "cdn": {
    "provider": "cloudflare",
    "cache": {
      "static": {
        "maxAge": 31536000
      },
      "dynamic": {
        "maxAge": 0
      }
    }
  }
}